{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "react-app"
  ],
  "plugins": [
    // "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
        // `style: true` 会加载 less 文件
      }
    ]
  ]
}
